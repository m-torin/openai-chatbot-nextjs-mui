{
  "extends": [
    "config:js-lib",
    ":dependencyDashboard",
    "group:monorepos",
    "group:fortawesome",
    "group:kubernetes",
    "group:rubyOnRails",
    "group:recommended",
    "workarounds:all"
  ],
  "enabledManagers": [
    "cloudbuild",
    "docker-compose",
    "dockerfile",
    "flux",
    "git-submodules",
    "github-actions",
    "helm-requirements",
    "helm-values",
    "helmfile",
    "helmsman",
    "helmv3",
    "homebrew",
    "html",
    "kubernetes",
    "nodenv",
    "npm",
    "nvm",
    "pre-commit",
    "regex",
    "ruby-version",
    "terraform-version",
    "terraform",
    "tflint-plugin"
  ],
  "major": {
    "automerge": false
  },
  "minor": {
    "automerge": true
  },
  "patch": {
    "automerge": true
  },
  "pin": {
    "automerge": true
  },
  "rangeStrategy": "bump",
  "labels": ["dependencies"],
  "packageRules": [
    {
      "matchPackagePatterns": ["strapi", "next"],
      "prPriority": 5
    },
    {
      "matchDatasources": ["go"],
      "groupName": "kubernetes packages",
      "groupSlug": "kubernetes-go",
      "matchPackagePrefixes": [
        "k8s.io/api",
        "k8s.io/apiextensions-apiserver",
        "k8s.io/apimachinery",
        "k8s.io/apiserver",
        "k8s.io/cli-runtime",
        "k8s.io/client-go",
        "k8s.io/cloud-provider",
        "k8s.io/cluster-bootstrap",
        "k8s.io/code-generator",
        "k8s.io/component-base",
        "k8s.io/controller-manager",
        "k8s.io/cri-api",
        "k8s.io/csi-translation-lib",
        "k8s.io/kube-aggregator",
        "k8s.io/kube-controller-manager",
        "k8s.io/kube-proxy",
        "k8s.io/kube-scheduler",
        "k8s.io/kubectl",
        "k8s.io/kubelet",
        "k8s.io/legacy-cloud-providers",
        "k8s.io/metrics",
        "k8s.io/mount-utils",
        "k8s.io/pod-security-admission",
        "k8s.io/sample-apiserver",
        "k8s.io/sample-cli-plugin",
        "k8s.io/sample-controller"
      ]
    },
    {
      "matchDepTypes": ["dependencies"],
      "prPriority": 1
    },
    {
      "matchDepTypes": ["devDependencies"],
      "matchPackagePatterns": ["lint", "prettier"],
      "automerge": true
    },
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    }
  ],
  "reviewers": [],
  "semanticCommitType": "chore",
  "docker": {
    "enabled": true
  },
  "node": {},
  "automergeType": "pr",
  "automergeStrategy": "squash",
  "lockFileMaintenance": {
    "enabled": true,
    "automerge": true,
    "automergeType": "pr",
    "platformAutomerge": true
  },
  "rollbackPrs": true,
  "ignoreTests": true,
  "configMigration": true,
  "prCreation": "not-pending",
  "prHourlyLimit": 20,
  "prConcurrentLimit": 3,
  "rebaseWhen": "conflicted",
  "respectLatest": true,
  "separateMultipleMajor": true,
  "separateMajorMinor": true,
  "stabilityDays": 2,
  "timezone": "America/New_York",
  "updateNotScheduled": false,
  "ignorePaths": [
    "**/__fixtures__/**",
    "**/__tests__/**",
    "**/examples/**",
    "**/node_modules/**",
    "**/test/**",
    "**/tests/**"
  ]
}
